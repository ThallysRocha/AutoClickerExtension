{
    "name": "Robô do Tinder",
    "version": "2.0.0",
    "description": "",
    "manifest_version": 3,
    "author": "Thallys Rocha",
    "host_permissions": ["<all_urls>"],
    "permissions": ["tabs"],
    "action":{
        "default_popup": "index.html",
        "default_title": "Robô do Tinder"
    },
    "icons":{
      "128": "icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ]
}